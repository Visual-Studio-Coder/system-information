{
  "name": "@tib/sn",
  "description": "A simple Node.js module for accessing the serial number of the local machine. Supports Linux, Mac (OS X), Windows, and FreeBSD.",
  "version": "0.2.2",
  "keywords": [
    "serial",
    "number",
    "serial-number",
    "sn",
    "hardware",
    "cpu",
    "uuid",
    "machine"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "copy:fixtures": "cpx \"./src/__tests__/fixtures/**/.*\" \"./dist/__tests__/fixtures\"",
    "acceptance": "tib-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "tib-tsc",
    "postbuild": "npm run copy:fixtures",
    "clean": "tib-clean tib-sn-cli*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "tib-mocha \"dist/__tests__/**/*.js\"",
    "unit": "tib-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf tib-sn-cli*.tgz && tree package && npm run clean"
  },
  "author": "Yvan <towyuan@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@tib/build": "^6.3.3",
    "@tib/eslint-config": "^8.2.3",
    "@tib/testlab": "^3.4.3",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "@types/tmp": "^0.2.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.0",
    "np": "^6.3.2",
    "tmp": "^0.2.1"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "cdcc79d9d061d463e8e21d1bda04a2f87a4a238f"
}
