{
  "name": "@libit/sn",
  "description": "A simple Node.js module for accessing the serial number of the local machine. Supports Linux, Mac (OS X), Windows, and FreeBSD.",
  "version": "0.3.2",
  "keywords": [
    "serial",
    "number",
    "serial-number",
    "sn",
    "hardware",
    "cpu",
    "uuid",
    "machine"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "Mindary",
  "license": "MIT",
  "scripts": {
    "copyfixtures": "cpx \"./src/__tests__/fixtures/**/.*\" \"./dist/__tests__/fixtures\"",
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "postbuild": "npm run copyfixtures",
    "clean": "lb-clean libit-sn*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs  \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha --allow-console-logs  \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf libit-sn*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.10",
    "@types/tmp": "^0.2.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "mocha": "^9.0.3",
    "np": "^7.5.0",
    "tmp": "^0.2.1"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "5b2f4c84ce347f43d8b2a7ebe594df1ab3bbcf01"
}
